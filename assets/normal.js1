    var elem = $("#tr-item-" + id);
    var quantity = elem.data("quantity");
    var price = elem.data("price");
    var price_taxes = elem.data("price-taxes");
    var price_without_vat = elem.data("price-without-vat")
    var currency = elem.data("currency");

    var sum = Math.round(price * quantity * 100) / 100;
    var sum_taxes = Math.round(price_taxes * quantity * 100) / 100;
    var sum_without_vat = Math.round(price_without_vat * quantity * 100) / 100;

    if(price != 0 && currency != 0) {
      $("#price-sum-" + id).html(sum + "&nbsp;" + currency);
      return [sum_taxes, currency, sum_without_vat];
    } else {
      return null;
    }

  }

  function calc_cart_sum() {
    var total_sum = {};
    var total_sum_without_vat = {};
    var vat = 9;

    $("#library_cart_table tbody .tr-item").each(function(tr) {
      var id = $(this).data("item-id");
      res = calc_item_sum(id);
      if(res != null) {
        var price = res[0];
        var price_without_vat = res[2]
        var currency = res[1];
        if(total_sum[currency] == null) {
          total_sum[currency] = 0;
          total_sum_without_vat[currency] = 0;
        }
        total_sum[currency] = total_sum[currency] + price;
        total_sum_without_vat[currency] = total_sum_without_vat[currency] + price_without_vat;
      }
    });
    sum_string = "";
    $.each(Object.keys(total_sum), function(){
      sum = Math.round(total_sum[this] * 100) / 100;
      sum_string = sum_string + sum + " " + this;
    });
    $("#cart_sum").html(sum_string);

    sum_without_vat_string = "";
    $.each(Object.keys(total_sum), function(){
      sum = Math.round(total_sum_without_vat[this] * 100) / 100;
      sum_without_vat_string = sum_without_vat_string + sum + " " + this;
    });
    $("#cart_sum_without_vat").html(sum_without_vat_string);

    sum_taxes = "";
    $.each(Object.keys(total_sum), function(){
      sum = Math.round((total_sum[this] - total_sum_without_vat[this]) * 100) / 100;
      sum_taxes = sum_taxes + sum + " " + this;
    });
    $("#cart_sum_taxes").html(sum_taxes);
