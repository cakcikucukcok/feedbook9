    $(".quantity-td a").click(function(evt) {
      evt.preventDefault();
      var parent_tr = $(this).parent().parent();
      var id = parent_tr.data("item-id");
      var quantity = parseInt(parent_tr.data("quantity"));
      url_data = $.url($(this).attr("href"));
      if (url_data.param("mod") == "more") {
        new_quantity = quantity + 1;
      } else if (url_data.param("mod") == "less"){
        new_quantity = quantity - 1;
      }

      if (url_data.param("quantity")) {
        new_quantity = url_data.param("quantity");
      }

      parent_tr.data("quantity", new_quantity);
      $("#quantity-" + id).attr("value", new_quantity);

      calc_cart_sum();

      if(time_out_id != 0) {
        window.clearTimeout(time_out_id);
      }
